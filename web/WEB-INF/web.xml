<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterStudent</servlet-name>
        <servlet-class>com.servlet.RegisterStudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginStudentServlet</servlet-name>
        <servlet-class>com.servlet.LoginStudentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentLogoutServlet</servlet-name>
        <servlet-class>com.servlet.StudentLogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveApplicationFormServlet</servlet-name>
        <servlet-class>com.servlet.SaveApplicationFormServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditApplicationFormServlet</servlet-name>
        <servlet-class>com.servlet.EditApplicationFormServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadImageServlet</servlet-name>
        <servlet-class>com.servlet.UploadImageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateImageServlet</servlet-name>
        <servlet-class>com.servlet.UpdateImageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterStudent</servlet-name>
        <url-pattern>/RegisterStudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginStudentServlet</servlet-name>
        <url-pattern>/LoginStudentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentLogoutServlet</servlet-name>
        <url-pattern>/StudentLogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveApplicationFormServlet</servlet-name>
        <url-pattern>/SaveApplicationFormServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditApplicationFormServlet</servlet-name>
        <url-pattern>/EditApplicationFormServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadImageServlet</servlet-name>
        <url-pattern>/UploadImageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateImageServlet</servlet-name>
        <url-pattern>/UpdateImageServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
